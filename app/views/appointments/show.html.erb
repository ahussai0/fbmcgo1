<div class="page-header">
  <h3>Appointment #<%= @appointment.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">

    <div class="col-xs-6"><%= @appointment.lead.firstname %> <%= @appointment.lead.lastname %><br>
      <%= @appointment.lead.address %><br>
      <%= @appointment.lead.city %>, <%= @appointment.lead.state %>  <%= @appointment.lead.zip %><br>
      <% if @lat != nil  %><img src="https://maps.googleapis.com/maps/api/staticmap?center=<%= @lat %>,<%= @long %>&zoom=17&size=400x400&key=AIzaSyCcx5KIGriBpoW_IVL6XPF0ATamX7zg8vc" alt=""><% end  %><br><br>
      

    </div>



    <dl class="dl-horizontal">
      <dt>Appt time</dt>
      <dd><%= @appointment.appt_time %></dd>

      <dt>Appt date</dt>
      <dd><%= @appointment.appt_date %></dd>

      <dt>Lead</dt>
      <dd><%= @appointment.lead_id %></dd>

      <dt>Agent</dt>
      <dd><%= @appointment.agent_id %></dd>

      <dt>Outcome</dt>
      <dd><%= @appointment.outcome %></dd>

      <dt>Notes</dt>
      <dd><%= @appointment.notes %></dd>

      <dt>Agency</dt>
      <dd><%= @appointment.agency_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/appointments" class="btn btn-primary">
        Back
      </a>
      <a href="/appointments/<%= @appointment.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_appointment/<%= @appointment.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
