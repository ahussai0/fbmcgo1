<div class="page-header">
  <h3>Agency #<%= @agency.id %> - <%= @agency.name %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Code</dt>
      <dd><%= @agency.code %></dd>

      <dt>Logo</dt>
      <dd><%= @agency.logo %></dd>

      <dt>Name</dt>
      <dd><%= @agency.name %></dd>

      <dt>Address</dt>
      <dd><%= @agency.address %></dd>

      <dt>Phone</dt>
      <dd><%= @agency.phone %></dd>

      <dt>City</dt>
      <dd><%= @agency.city %></dd>

      <dt>State</dt>
      <dd><%= @agency.state %></dd>

      <dt>Zip</dt>
      <dd><%= @agency.zip %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/agencies" class="btn btn-primary">
        Back
      </a>
      <a href="/agencies/<%= @agency.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_agency/<%= @agency.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
<h3>Current Agents</h3>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Code</th>
        <th>License #</th>
        <th>Lead Count</th>
        <th>Appointment Count</th>
        <th>Photo</th>

      </tr>

      <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.firstname %> <%= agent.lastname %></td>
        <td><%= agent.code %></td>
        <td><%= agent.license %></td>
        <td><%= agent.leads.count %></td>
        <td><%= agent.appointments.count %></td>
        <td><%= agent.jpg %></td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
<h3>Current Leads</h3>



<table class="table table-striped table-hover"
  <tr>
    <th>Date</th>
    <th>Code</th>
    <th>Name</th>
    <th>Status</th>
    <th>Agent</th>
    <th>Agency</th>>
    <th>Actions</th>
  </tr>

  <% @leads.each do |lead| %>
  <tr>
    <td><%= lead.created_at.to_date %></td>
    <td><%= lead.dw_agn %> <%= lead.batchno %>-<%= lead.recno %></td>
    <td><%= lead.firstname %> <%= lead.lastname %></td>
    <td><%= lead.status %></td>
    <td><%= lead.agent.code %></td>
    <td><%= lead.agency.code %></td>



    <td>
      <a href="/leads/<%= lead.id %>" class="btn btn-primary">Show</a>
      <a href="/leads/<%= lead.id %>/edit" class="btn btn-warning">Edit</a>
      <a href="/delete_lead/<%= lead.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
  
