<div class="page-header">
  <h1>
    Agencies
    <a href="/agencies/new" class="btn btn-lg btn-success">New Agency</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Code</th>
        <th>Logo</th>
        <th>Name</th>
        <th>Address</th>
        <th>Phone</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th>Actions</th>
      </tr>

      <% @agencies.each do |agency| %>
      <tr>
        <td><%= agency.code %></td>
        <td><%= agency.logo %></td>
        <td><%= agency.name %></td>
        <td><%= agency.address %></td>
        <td><%= agency.phone %></td>
        <td><%= agency.city %></td>
        <td><%= agency.state %></td>
        <td><%= agency.zip %></td>
        <td>
          <a href="/agencies/<%= agency.id %>" class="btn btn-primary">Show</a>
          <a href="/agencies/<%= agency.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_agency/<%= agency.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
