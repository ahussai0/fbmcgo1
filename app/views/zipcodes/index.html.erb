<div class="page-header">
  <h1>
    Zipcodes
    <a href="/zipcodes/new" class="btn btn-lg btn-success">New Zipcode</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Zipcode</th>
        <th>Agency</th>
        <th>County</th>
        <th>State</th>
        <th>Agent</th>
        <th>Actions</th>
      </tr>

      <% @zipcodes.each do |zipcode| %>
      <tr>
        <td><%= zipcode.zipcode %></td>
        <td><%= zipcode.agency_id %></td>
        <td><%= zipcode.county %></td>
        <td><%= zipcode.state %></td>
        <td><%= zipcode.agent_id %></td>
        <td>
          <a href="/zipcodes/<%= zipcode.id %>" class="btn btn-primary">Show</a>
          <a href="/zipcodes/<%= zipcode.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_zipcode/<%= zipcode.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
