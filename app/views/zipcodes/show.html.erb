<div class="page-header">
  <h3>Zipcode #<%= @zipcode.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Zipcode</dt>
      <dd><%= @zipcode.zipcode %></dd>

      <dt>Agency</dt>
      <dd><%= @zipcode.agency_id %></dd>

      <dt>County</dt>
      <dd><%= @zipcode.county %></dd>

      <dt>State</dt>
      <dd><%= @zipcode.state %></dd>

      <dt>Agent</dt>
      <dd><%= @zipcode.agent_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/zipcodes" class="btn btn-primary">
        Back
      </a>
      <a href="/zipcodes/<%= @zipcode.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_zipcode/<%= @zipcode.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
